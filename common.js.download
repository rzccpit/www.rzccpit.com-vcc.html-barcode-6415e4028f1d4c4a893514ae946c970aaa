function commonAjaxLeast(url, formdata, type, async, callback, adminFlag,signSalt) {
    var headers = getAjaxHeader(adminFlag,signSalt);
    $.ajax({
        url: url,
        headers: headers,
        type: type,
        dataType: "json",
        data: type == 'post' ? JSON.stringify(formdata) : formdata,
        // 告诉jQuery不要去处理发送的数据
        processData: type == "post" ? false : true,
        // 告诉jQuery不要去设置Content-Type请求头,post请求formdata数据需用JSON.stringify()进行转码
        contentType: type == "post" ? "application/json;charset=UTF-8" : 'application/json',
        async: async,
        cache: false,
        success: function (data) {
            if (data.code == 200 || data.code == 0) {
                // 回调
                if (callback && typeof (callback) == 'function') callback(data);
            } else if (data.code == 530) {
                //todo 重新登录
