function filterAjax(url,method){
	if(url!='' && url!=null && url!=undefined){
	//	url后有参数要去掉
		let parIdx=url.indexOf('?')
		if(parIdx>-1){
			url=url.substring(0,parIdx)
		}
		
		let curUrl
		if(url.indexOf('/ccpitcompany')>-1){
			let idx=url.lastIndexOf('/ccpitcompany')+13
			
			curUrl=url.substring(idx,url.length)
		}else if (url.indexOf('www.rzccpit.com')>-1){
			let idx=url.lastIndexOf('www.rzccpit.com')+15
			curUrl=url.substring(idx,url.length)
		}else{
	//		有的页面没有加COMMONPATH
			if(url.indexOf('../')>-1){
				url=url.replace('../','')
			}
			curUrl='/'+url
			url=COMMONPATH+'/'+url
			
		}
		
		
		let token=getCookie("enterprisekey")
		var sign_key
		if(!!token){
			let useid=getCookie("enterpriseid")
			curUrl=curUrl.replace('api/','applet/')
			url=url.replace('api/','applet/')
			 var paramsA=useid+changeDateFormat(new Date(),'YYYY-MM-DD')+"userpc"+"jiangsuhailonghuasi"+curUrl+changeDateFormat(new Date(),'YYYY-MM-DD')+token
		       
		     var result=md5(paramsA)
		     sign_key=result.toLocaleUpperCase()
		}else{
			var tmpurljm=url
			curUrl=curUrl.replace('pc/','noapi/')
			url=url.replace('pc/','noapi/')
			
			 if(method=="get"||method=="GET"){
				 tmpurljm=curUrl.split("?")[0]
	          }else{
	            tmpurljm=curUrl
	          }
			var paramsB=changeDateFormat(new Date(),'YYYY-MM-DD')+"userpc"+"jiangsuhailonghuasi"+tmpurljm+changeDateFormat(new Date(),'YYYY-MM-DD')
		     
			var resultB=md5(paramsB)
	        sign_key=resultB.toLocaleUpperCase()
		}
		let obj={sign_key:sign_key,url:url}
		return obj
	}
}
