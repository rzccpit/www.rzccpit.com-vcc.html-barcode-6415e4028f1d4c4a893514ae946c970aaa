/**
 *  返回元素在数组中的下标
 *  @param {string} str 查询的元素（必传）
 *  @param {array} arr 数组（必传）
 *  @param {string} key 键值（当数组每项为对象时必传）
 */
function searchArrIndex(str, arr, key) {
	for (var i = 0; i < arr.length; i++) {
		if (!!key) {
			if (arr[i][key] == str) {
				return i;
			}
		} else {
			if (arr[i] == str) {
				return i;
			}
		}
	}
	return -1;
}

