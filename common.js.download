 * callback：回调函数
 */
function commonAjax(url, formdata, type, async, callback) {
	let obj = filterAjax(url,type)
	// 判断两次点击间隔
	if(GetDateTimeDiff(url)) {
		return;
	}
	formdata.append("sign_key", obj.sign_key)
	formdata.append("admintype", "userpc");
	log("AJAX请求开始。");
	log("AJAX请求地址：" + url);
	$.ajax({
		url: obj.url,
		type: type,
		dataType: "json",
		data: formdata,
		// 告诉jQuery不要去处理发送的数据
		processData: false,
		// 告诉jQuery不要去设置Content-Type请求头
		contentType: false,
		async: async,
		cache: false,
		success: function(data) {
			log("AJAX请求成功。");
			if(checkError(data)) return false;
			if(data != null) {
				// 判断是否成功
				if(data.key == "SUCCESS") {
					// 回调
					if(callback && typeof(callback) == 'function') callback(data);
				} else {
					alertWarning(data.msg);
				}
			} else {
				alertWarning("服务器没有返回数据，请联系管理员！");
			}
		},
		error: function(XMLHttpRequest, textStatus, errorThrown) {
			console.log("AJAX请求失败。"+url);
			errorLogin(XMLHttpRequest, textStatus, errorThrown);
		}
	});
}

