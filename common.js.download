/*
 * 14位编号按6,3,5加空格分隔
 */
function splitBySpace(number) {
	return number;
	if(number.length == 14) {
		var firstnum = number.substr(0, 6);
		var secondnum = number.substr(6, 3);
		var thirdnum = number.substr(9, 5);
		return(firstnum + " " + secondnum + " " + thirdnum);
	} else {
		return number;
	}
}
