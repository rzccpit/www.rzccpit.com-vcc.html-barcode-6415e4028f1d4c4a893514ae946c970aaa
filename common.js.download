function pwdyz(pwdstr){
	
	var reg1=/([0-9])/;
	var reg2=/([A-Z])/;
	var reg3=/([a-z])/;
	var reg4=/([!#@*])/;
	if(reg1.test(pwdstr)&&reg2.test(pwdstr)&&reg3.test(pwdstr)&&reg4.test(pwdstr)){
		return true;
	}else{
		return false;
	}
}


